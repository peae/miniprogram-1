  "sitemapLocation": "sitemap.json",
  "darkmode": true,
  "themeLocation": "theme.json",
  "useExtendedLib": {
    "kbone": true,
    "weui": true
  },
  "usingComponents": {
    "mp-form": "weui-miniprogram/form/form"
  }
